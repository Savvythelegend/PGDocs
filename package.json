{
  "name": "my-website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "fix-mdx": "node scripts/fix-mdx.js",
    "convert-tables": "node scripts/convert-tables.js",
    "convert-tables:docs": "node scripts/convert-tables.js docs/",
    "convert-tables:backup": "node scripts/convert-tables.js docs/ --backup",
    "format": "prettier --write \"docs/**/*.mdx\"",
    "format:check": "prettier --check \"docs/**/*.mdx\"",
    "lint:mdx": "remark docs --ext .mdx"
